    "import random\n",
    "import string\n",
    "\n",
    "all_char = string.ascii_letters + string.digits + string.punctuation\n",
    "\n",
    "a=int(input(\"Length of password between 4 and 8: \"))\n",
    "\n",
    "if a>8 or a<4:\n",
    "    print(\"Error: Enter password length between 4 and 8\")    \n",
    "else:\n",
    "    print(''.join(random.sample(all_char,k=a)))"
